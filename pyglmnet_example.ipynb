     "output_type": "stream",
     "text": [
      "7311.98687185 7704.20399676\n"
     ]
    }
   ],
   "source": [
    "# Compute model deviance\n",
    "Dr = model.deviance(yr, yrhat)\n",
    "Dt = model.deviance(yt, ythat)\n",
    "print Dr, Dt"
   ]
